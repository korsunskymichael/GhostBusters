<h2>TODO LIST:</h2>

<table id=table>   
    <tr>
        <th>ID</th>
        <th>Assignee</th>
        <th>Description</th> 
        <th>Finish Date</th>
        <th>Completed?</th>
        <% rows.forEach(function(row) { %>
            <tr >
                <td>
                    <%= row.todo_id %>
                </td>  
                <td>
                    <%= row.assignee %>
                </td>    
                
                <td>
                    <%= row.description %> 
                </td>
                
                <td>
                    <%= row.finish_date %>
                </td>
                
            </tr>
        <% }); %>
    
    
    </tr>
    
</table> 



